{
  "expo": {
    "name": "hog-app",
    "slug": "hog-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d1d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jardacoder.hogapp"
    },
    "android": {
      "package": "com.jardacoder.hogapp",
      "googleServicesFile" : "./google-services.json" 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
